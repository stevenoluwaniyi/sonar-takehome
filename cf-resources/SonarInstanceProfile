
Resources:
    SonarProfile:
        Type: AWS::IAM::InstanceProfile
        Properties: 
            InstanceProfileName: SonarProfile
            Roles: 
                - ecsInstanceRole

Outputs:
    ProfileArn:
      Description: ECSCluster based on AZ 
      Value: !GetAtt SonarProfile.Arn
      Export:
        Name: !Sub "${AWS::StackName}-Arn"
